meme_dict = {
            "CRINGE": "Garip ya da utandırıcı bir şey",
            "LOL": "Komik bir şeye verilen cevap",
            "XD" : "Bir şeyi gülünç bulma durumu",
            "Flex": "Gösteriş yapmak",
            "Vibe": "Bir kişinin üzerinde bıraktığı izlenim",
            "Ghosting": "Birine aniden cevap vermemek"
}
word = input("Anlamadığınız bir kelime yazınız (hepsini büyük harflerle giriniz!): ")
if word in meme_dict.keys():
        print(meme_dict[word])    
else:
    # Kelime eşleşmiyorsa ne yapmalıyız? 
    print("Bu Kelime Sözlükte Yok!!!")
